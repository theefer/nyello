PATTERNREF, MATCH, SEQUENCE, OPERATION
parentheses
+



OPERATION := UNARYOP __ | __ BINARYOP __

UNARYOP  := not
BINARYOP := and | or | xor


PATTERNREF := - | -NUMBER


MATCH := STRINGFLAG STRINGVALUE | COMPAREFLAG COMPAREVALUE | STRINGVALUE

STRINGFLAG := SHORTSTRINGFLAG | LONGSTRINGFLAG
SHORTSTRINGFLAG := -a       | -l      | -t      | -g      | -z
LONGSTRINGFLAG  := --artist | --album | --title | --genre | --any

COMPAREFLAG := SHORTCOMPAREFLAG | LONGCOMPAREFLAG
SHORTCOMPAREFLAG := -y     | -n
LONGCOMPAREFLAG  := --year | --track

STRINGVALUE := <string>
COMPAREVALUE := COMPAREOP_NUMBER
COMPAREOP := <empty> | < | <= | = | => | >


SEQUENCE := PLAYLISTSEQ | LASTPATSEQ | MLIBSEQ
PLAYLISTSEQ := PLAYLISTNAME/IDSEQ
LASTPATSEQ := #IDSEQ
MLIBSEQ := IDSEQ

PLAYLISTNAME := <empty> | <playlist-name>
IDSEQ := IDRANGE | IDRANGE,IDRANGE
IDRANGE := NUMBER | NUMBER- | -NUMBER | NUMBER-NUMBER
NUMBER := [0-9]+
